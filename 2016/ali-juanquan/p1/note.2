
# record all encryption

def purify(s):
    return ''.join(s.split())

def encrypt(key, msg):
    msg = unhex(msg)
    key = unhex(key)
    return enhex(AES.new(key, mode=AES.MODE_CBC, IV=key).encrypt(msg))

def decrypt(key, encrypted):
    encrypted = unhex(encrypted)
    key = unhex(key)
    return enhex(AES.new(key, mode=AES.MODE_CBC, IV=key).decrypt(encrypted))


